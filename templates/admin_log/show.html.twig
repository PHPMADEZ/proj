{% extends 'base.html.twig' %}

{% block title %}AdminLog{% endblock %}

{% block body %}
    <h1>AdminLog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ admin_log.id }}</td>
            </tr>
            <tr>
                <th>Naam</th>
                <td>{{ admin_log.naam }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ admin_log.description }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ admin_log.user.name }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ admin_log.date ? admin_log.date|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_log_index') }}">back to list</a>

    <a href="{{ path('app_admin_log_edit', {'id': admin_log.id}) }}">edit</a>

    {{ include('admin_log/_delete_form.html.twig') }}
{% endblock %}
